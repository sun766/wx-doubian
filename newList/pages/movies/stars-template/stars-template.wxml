<template name="starTmp">
    <view class="star-container">
      <view class="starList">
        <block wx:for="{{5}}" wx:for-item="item" wx:key="index">
           <image wx:if="{{ (index+1) - stars/10<0}}" src="/images/icon/star.png"></image>
           <image wx:else src="/images/icon/none-star.png"></image>
        </block>
      </view>
      <text class="score">{{average}}</text>
    </view>
</template>